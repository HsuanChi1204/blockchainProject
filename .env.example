# Example .env file for blockchain project
# Fill in your own values and rename to .env

# Metamask private key (for deploying/interacting with contracts)
PRIVATE_KEY=your_metamask_private_key_here

# Pinata API credentials (for uploading to IPFS)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_API_SECRET=your_pinata_api_secret_here

# Cardona zkEVM RPC URL (testnet or mainnet)
CARDONA_RPC_URL=https://rpc.cardona.zkevm-rpc.com